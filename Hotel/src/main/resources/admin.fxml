<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="160.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller_log">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="350" minWidth="10.0" prefWidth="350.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Button mnemonicParsing="false" text="Pokaż listę pracowników" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="0" onAction="#lista_pracownikow"/>
        <Button mnemonicParsing="false" text="Dodaj pracownika" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="1" onAction="#dodaj_pracownika"/>
        <Button mnemonicParsing="false" text="Usuń pracownika" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.rowIndex="2" onAction="#usun_pracownika"/>
    </children>
</GridPane>

